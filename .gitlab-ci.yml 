image: maven:3.6.3-jdk-13

cache:
  paths:
    - ~/.m2/repository

stages:
  - build

build_branch_feature:
  stage: build
  script:
    - mvn clean install -Dskip.integration.tests=false -Dskip.unit.tests=false
  only:
    variables:
      - $CI_SERVER_HOST == "gitlab.com"
  except:
    refs:
      - master